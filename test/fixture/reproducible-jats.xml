<!DOCTYPE article PUBLIC "-//NLM//DTD JATS (Z39.96) Journal Archiving and Interchange DTD v1.1d3 20150301//EN"  "JATS-archivearticle1.dtd">
<article xmlns:xlink="http://www.w3.org/1999/xlink">
  <body>
    <!-- Stencila Mini Cell -->
    <code specific-use="cell" id="cell1">
      <named-content><alternatives>
        <code specific-use="source" language="mini"><![CDATA[x=5]]></code>
        <code specific-use="output" language="json"><![CDATA[{"execution_time": 1, "value_type": "number", "value": 5 }]]></code>
      </alternatives></named-content>
    </code>
    <!-- Stencila Local Context Cell -->
    <code specific-use="cell" id="cell2">
      <named-content><alternatives>
        <code specific-use="source" language="mini"><![CDATA[node(x)]]></code>
        <code specific-use="source" language="node"><![CDATA[process.title + ' ' + process.version + ' x=' + x.toString() ]]></code>
        <code specific-use="output" language="json"><![CDATA[{"execution_time": 1, "value_type": "string", "value": 'node v6.9.2 x=5' }]]></code>
      </alternatives></named-content>
    </code>
    <!-- Jupyter Global Context Cell -->
    <code specific-use="cell" id="cell3">
      <named-content><alternatives>
        <code specific-use="source" language="node"><![CDATA[process.title + ' ' + process.version ]]></code>
        <code specific-use="output" language="json"><![CDATA[{"execution_time": 1, "value_type": "string", "value": 'node v6.9.2' }]]></code>
      </alternatives></named-content>
    </code>
    <!-- Reproducible figure -->
    <fig id="f1" fig-type="repro-fig">
      <caption>
        <title>Biodiversity on Mars</title>
        <p>Lorem ipsum</p>
      </caption>
      <alternatives>
        <code specific-use="source" language="mini"><![CDATA[scatter3d(co_occurence_matrix)]]></code>
        <code specific-use="output" language="json"><![CDATA[{"execution_time": 1, "value_type": "plot-ly", "value": {} }]]></code>
      </alternatives>
      <graphic specific-use="output" xlink:href="89f8b53e361f.png"/>
    </fig>
  </body>
</article>
